pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                sh 'make'
                archiveArtifacts artifacts: '**/reports/HarReport/*.har', fingerprint: true
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}