node {
tool name: 'maven', type: 'maven'
tool name: 'jdk', type: 'jdk'
    stage('checkout code') {
        checkout scm
    }
    stage('Build') {
        echo 'Building...'
    }
    stage('Test') {
        echo 'Testing...'
        maven {
        sh 'mvn test'
        }
    }
    stage('Deploy') {
        echo 'Deploying...'
    }
}